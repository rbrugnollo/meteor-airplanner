{
  "name": "meteor-airplanner",
  "private": true,
  "scripts": {
    "start": "meteor --exclude-archs web.browser.legacy",
    "debug": "meteor --exclude-archs web.browser.legacy --inspect",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "lint": "eslint  --ignore-path .eslintignore **/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --ignore-path .eslintignore --fix '**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,md,json}' --config .prettierrc",
    "before-commit": "npm run format && npm run lint:fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.10",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.8",
    "@mui/material": "^5.14.8",
    "@react-icons/all-files": "^4.1.0",
    "@zodern/babel-plugin-meteor-relay": "^1.1.3",
    "formik": "^2.4.4",
    "framer-motion": "^6.5.1",
    "lodash": "^4.17.21",
    "material-react-table": "^1.14.0",
    "meteor-node-stubs": "^1.2.5",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "simpl-schema": "^3.4.1",
    "yup": "^1.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/meteor": "^2.9.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/react": "^18.2.20",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    },
    "testModule": "tests/main.ts"
  }
}
